    <!-- <%- member.gsoc_project %> -->
    Project: <a href="<%- member.gsoc_project_link %>" target="_blank"><%- member.gsoc_project %></a>
    <br>
<% } %>
<% if (member.gsoc_report) { %>
    <a class="external" href="<%- member.gsoc_report %>" target="_blank">Contributions & Report</a>
